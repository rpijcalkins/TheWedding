@model Dictionary<string, List<string>>
@{
    ViewBag.Title = "Pictures";
}

<div class="row">
    <div class="col-lg-12">
        <p>Photos will be posted here as we get them.</p>
    </div>
</div>
@foreach (var group in Model)
{
<div class="row">
    <div class="col-lg-12">
        <h4>@group.Key</h4>
    </div>
</div>
<div class="row">
    @foreach (var pic in group.Value)
    {
    <div class="col-xs-6 col-sm-4 col-md-3 col-lg-2">@pic</div>
    }
</div>
}